<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.admin.starwingsapp">

    <permission
        android:name="com.example.admin.starwingsapp.permission.C2D_MESSAGE"
        android:protectionLevel="signature" />

    <uses-permission android:name="com.example.admin.starwingsapp.permission.C2D_MESSAGE" />
    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <uses-permission android:name="com.google.android.c2dm.permission.RECEIVE" />
    <uses-permission android:name="android.permission.WAKE_LOCK" />

    <application
        android:name=".MyApplication"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".AboutUsActivity"
            android:parentActivityName=".Dashboard" />
        <activity android:name=".SplashScreen">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".NotificationActivity" />
        <receiver
            android:name="com.google.android.gms.gcm.GcmReceiver"
            android:exported="true"
            android:permission="com.google.android.c2dm.permission.SEND">
            <intent-filter>
                <action android:name="com.google.android.c2dm.intent.RECEIVE" />
                <action android:name="com.google.android.c2dm.intent.REGISTRATION" />
                <category android:name="com.example.admin.starwingsapp" />
            </intent-filter>
        </receiver>
        <service
            android:name=".MyGcmListenerService"
            android:exported="false">
            <intent-filter>
                <action android:name="com.google.android.c2dm.intent.RECEIVE" />
            </intent-filter>
        </service>
        <service
            android:name=".MyInstanceIDListenerService"
            android:exported="false">
            <intent-filter>
                <action android:name="com.google.android.gms.iid.InstanceID" />
            </intent-filter>
        </service>
        <service
            android:name=".MyGCMRegistrationIntentService"
            android:exported="false">
        </service>
        <activity
            android:name=".OurCourses"
            android:label="Our Courses">
            <intent-filter>
                <action android:name="com.example.admin.starwingsapp.OURCOURSES" />

                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>
        <activity
            android:name=".Dashboard"
            android:launchMode="singleTop" />
        <activity
            android:name=".ContactInfoActivity"
            android:parentActivityName=".Dashboard" />
        <activity
            android:name=".BatchesActivity"
            android:launchMode="singleTop" />
        <activity
            android:name=".ACCACourseActivity"
            android:parentActivityName=".BatchesActivity" />
        <activity
            android:name=".CAActivity"
            android:parentActivityName=".BatchesActivity" />
        <activity
            android:name=".ExcelCourseActivity"
            android:parentActivityName=".BatchesActivity" />
        <activity
            android:name=".GSTCourseActivity"
            android:parentActivityName=".BatchesActivity" />
        <activity android:name=".CenterActivity" />
        <activity android:name=".Downloads" />
        <activity android:name=".TopicsActivity" />
        <activity android:name=".ChapterActivity" />
        <activity android:name=".CourseActivity" />
        <activity
            android:name=".Login"
            android:windowSoftInputMode="stateHidden|adjustResize" />
        <activity
            android:name=".Support"
            android:windowSoftInputMode="stateHidden|adjustResize" />
        <activity android:name=".VideosListActivity"/>
        <activity android:name=".VideoActivity" />
        <activity android:name=".ResourcesActivity" />
        <activity android:name=".ProfileActivity" />
        <activity android:name=".PapersActivity"/>
        <activity android:name=".PapersDisplayActivity"/>
        <activity android:name=".DailyScheduleActivity"></activity>
    </application>

</manifest>